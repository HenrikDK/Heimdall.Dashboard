<!DOCTYPE html>
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link rel="icon" href="/art/viking-ship.svg">
    
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/tabulator-tables@5.3.4/dist/css/tabulator_semanticui.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/css/bootstrap.min.css">
    
    <script src="https://cdn.jsdelivr.net/npm/vue@3.2.37"></script>
    <script src="https://cdn.jsdelivr.net/npm/vue-router@4.1.5"></script>
    <script src="https://cdn.jsdelivr.net/npm/echarts@5.3.3"></script>
    <script src="https://cdn.jsdelivr.net/npm/vue-echarts@6.1.0"></script>
    <script src="https://cdn.jsdelivr.net/npm/moment@2.29.3"></script>
    <script src="https://cdn.jsdelivr.net/npm/tabulator-tables@5.3.4"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1"></script>
    
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <script src="~/js/site.js" asp-append-version="true"></script>
</head>
<body>

<div id="app">
    @RenderBody()
</div>

@RenderSection("VueComponents", required: false)

@RenderSection("Scripts", required: false)

<script>
    const router = VueRouter.createRouter({
      history: VueRouter.createWebHashHistory(),
      routes
    })

    const app = Vue.createApp({
    methods: {},
    mixins: mixinArray,
    data() {
        return {
            view: "cluster",
            namespace: "",
            search: "",
        }
    }
    })
    .component('v-chart', VueECharts);
    Object.entries(components).forEach(([k,v]) =>{
        app.component(k, v)
    });
    app.use(router)
    app.mount('#app');
</script>
<style>
  .bd-layout
  {
    min-height: calc(100vh - 55px);
    display:grid;
    gap:.5rem;
    grid-template-areas:"sidebar main";
    grid-template-columns:80px auto
  }
</style>
</body>
</html>